# Database PostgreSQL
POSTGRES_USER=seu-usuario-aqui
POSTGRES_PASSWORD=sua-senha-aqui
POSTGRES_DB=seu-banco-aqui
POSTGRES_NON_ROOT_USER=seu-usuario-nao-root-aqui
POSTGRES_NON_ROOT_PASSWORD=sua-senha-nao-root-aqui

# Encryption (mantenha a mesma que está atualmente no container editor)
ENCRYPTION_KEY=sua-chave-de-encriptacao-aqui

# Domain and access
N8N_HOST=seu-dominio-aqui
N8N_PORT=5678
N8N_PROTOCOL=https

# Queue mode settings
EXECUTIONS_MODE=queue
QUEUE_BULL_REDIS_TIMEOUT_THRESHOLD=10000
QUEUE_WORKER_CONCURRENCY=5
QUEUE_HEALTH_CHECK_ACTIVE=true
N8N_DISABLE_PRODUCTION_MAIN_PROCESS=true

# Performance and scaling
N8N_CONCURRENCY_PRODUCTION_LIMIT=50
EXECUTIONS_TIMEOUT=3600
EXECUTIONS_TIMEOUT_MAX=7200
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=336
EXECUTIONS_DATA_PRUNE_MAX_COUNT=50000

# Metrics and monitoring
N8N_LOG_LEVEL=info
N8N_METRICS=true
N8N_METRICS_INCLUDE_QUEUE_METRICS=true

# Security and settings
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
N8N_TRUSTED_PROXIES=*
N8N_PUSH_BACKEND=websocket
N8N_TRUST_PROXY=true

# Task Runners
N8N_RUNNERS_ENABLED=true
OFFLOAD_MANUAL_EXECUTIONS_TO_WORKERS=true

# Regional settings
GENERIC_TIMEZONE=America/Sao_Paulo

